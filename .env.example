MONGODB_HOST=127.0.0.1
MONGODB_PORT=27017
MONGODB_DATABASE=default
MONGODB_USERNAME=root
MONGODB_PASSWORD=password

MONGO_EXPRESS_USERNAME=root
MONGO_EXPRESS_PASSWORD=password
MONGO_EXPRESS_PORT=8081

# Cassandra
